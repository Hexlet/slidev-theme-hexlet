{
  "name": "slidev-theme-hexlet",
  "version": "0.0.5",
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      },
      "hightlighter": "prism"
    }
  },
  "scripts": {
    "dev": "slidev example.md --open",
    "build": "slidev build example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/theme-default": "^0.19.1",
    "@slidev/types": "^0.22.1",
    "@vueuse/motion": "^1.5.6",
    "codemirror-theme-vars": "^0.1.1",
    "lodash": "^4.17.21",
    "npm-check-updates": "^11.8.1",
    "prism-theme-vars": "^0.2.2",
    "release-it": "^14.10.0",
    "sass": "^1.35.1",
    "theme-vitesse": "^0.1.12"
  },
  "devDependencies": {
    "@slidev/cli": "^0.22.1",
    "@types/lodash": "^4.14.171"
  }
}
